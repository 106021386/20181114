import java.util.Scanner;

public class p5 {
    public static void main(String[] args) {
        Scanner scn = new Scanner(System.in);
    int a = scn.nextInt();
    char c =scn.next().charAt(0);
    for(int i =1;i<=a;i++){
        for(int j =1; j<=a;j++){
            System.out.print(c);
        }
        System.out.println();
    }
    }
}
